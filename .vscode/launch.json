{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "dipole",
      "type": "python",
      "request": "launch",
      "stopOnEntry": false,
      "pythonPath": "${config:python.pythonPath}",
      "program": "${workspaceRoot}/cccbdb.py",
      "cwd": "${workspaceRoot}",
      "env": {},
      "args": [
        "dipole",
        "CH4"
      ],
      "envFile": "${workspaceRoot}/.env",
      "debugOptions": [
        "WaitOnAbnormalExit",
        "WaitOnNormalExit",
        "RedirectOutput"
      ]
    },
    {
      "name": "geom",
      "type": "python",
      "request": "launch",
      "stopOnEntry": false,
      "pythonPath": "${config:python.pythonPath}",
      "program": "${workspaceRoot}/cccbdb.py",
      "cwd": "${workspaceRoot}",
      "env": {},
      "args": [
        "geom",
        "CH4"
      ],
      "envFile": "${workspaceRoot}/.env",
      "debugOptions": [
        "WaitOnAbnormalExit",
        "WaitOnNormalExit",
        "RedirectOutput"
      ]
    }
  ]
}